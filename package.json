  "bugs": {
    "url": "https://github.com/Jirkasa/algoritmy-a-datove-struktury-v-js/issues"
  },
  "homepage": "https://github.com/Jirkasa/algoritmy-a-datove-struktury-v-js#readme",
  "devDependencies": {
    "autoprefixer": "^10.3.7",
    "less": "^4.1.2",
    "nodemon": "^2.0.15",
    "postcss": "8.3.6",
    "postcss-cli": "8.3.1",
    "rollup": "^2.62.0"
  },
  "dependencies": {
    "regenerator-runtime": "^0.13.9"
