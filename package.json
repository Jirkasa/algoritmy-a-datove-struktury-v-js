  "description": "Website about algorithms and data structures in JavaScript.",
  "main": "index.js",
  "scripts": {
    "compile:less": "lessc ./less/main.less ./media/css/style.css",
    "nodemon:compile:less": "nodemon -e less -x \"npm run compile:less\"",
