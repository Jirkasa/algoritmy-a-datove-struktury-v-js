    "bundle:modules": "rollup ./media/js/modules/main.js --file ./media/js/interactiveExample.js --format iife"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jirkasa/algoritmy-a-datove-struktury-v-js.git"
  },
